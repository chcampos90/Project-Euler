# um numero primo tem somente dois
#fatores 1 e a si mesmo.
divisores = []
num = 1;
soma = 0
primo_master = 0
num3 = 1;

while soma + num < 1000000:
    #verifica quem pode dividir o numero
    for n in range(1, 1000000+1):
        if num % n == 0:
            #adiciona ao factores
            divisores.append(n)
            #verifica se tem mais de dois divisores
            if len(divisores) > 2:
                num = num + 1
                divisores.clear()
                break
    #verifica se o numero de divisores Ã© igual a dois ( por ele meso e 1)
    if len(divisores) == 2:
        #soma
        soma = soma + num
        num = num + 1
        divisores.clear()
        print('teste {}'.format(soma))

#inicia busca pelo maior primo
while primo_master == 0 or num3 < 1000000:

    #busca o menor NUMERO PRIMO
    for n in range(1, 1000001):
        if num3 % n == 0:
            divisores.append(n)
            if len(divisores) > 2:
                num3 = num3 + 1
                divisores.clear()
                break
    if len(divisores) == 2:
        divisores.clear()
        num4 = num3
        num3 = num3 + 1

        #verifica se a SOMA - o menor numero primo encontrado gera um resultado com numero primo
        for n in range(1,  soma + 1):
            if (soma - num4) % n == 0:
                divisores.append(n)
                if len(divisores) > 2:
                    divisores.clear()
                    break
        if len(divisores) == 2:
            #sendo primo passa o valor para o PRIMO_MASTER
            primo_master = soma - num4
            divisores.clear()
            break

print('RESULTADO: {} '.format(primo_master))
